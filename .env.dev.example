# Development Environment Configuration

# Environment
ENVIRONMENT=dev

# Ollama Configuration
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama3:latest
OLLAMA_TIMEOUT=300

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=true
API_RELOAD=true

# Database (if needed)
DATABASE_URL=sqlite:///./dev.db

# Logging
LOG_LEVEL=DEBUG
LOG_FORMAT=detailed

# Feature Flags
ENABLE_CACHE=false
ENABLE_METRICS=true
